{{> header }}
<body>
    <div class="wrapper">
        <header>
            {{> navbar type=true}}
        </header>
        <article>
            {{#each collections.posts}}
            <a href="{{this.alias}}/" class="entry-link">
                <div class="entry">
                    {{ this.title }}
                </div>
            </a>
            {{/each}}
        </article>
        <div class="push"></div>
    </div>
    <footer>
    </footer>
</body>
</html>